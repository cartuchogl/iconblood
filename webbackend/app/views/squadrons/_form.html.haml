- form_for @squadron do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :player_id
    %br
    = f.text_field :player_id
  %p
    = f.label :game_id
    %br
    = f.text_field :game_id
  %p
    = f.label :image_id
    %br
    = f.text_field :image_id
  %p
    = f.submit
